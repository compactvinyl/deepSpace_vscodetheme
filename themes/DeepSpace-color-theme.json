{
	"name": "DeepSpace",
	"type": "dark",
	"author": { "name" : "twopill" },
	"colors": {
		"editor.background": "#252525",
		"editor.foreground": "#f1ffee",
		"activityBarBadge.background": "#dd4c35",
		"activityBar.activeBackground": "#252525",
		"sideBarTitle.foreground": "#dd4c35",
		"activityBar.background": "#252525",
		"activityBar.activeBorder": "#dd4c35",
		"window.inactiveBorder": "#f1ffee3b",
		"titleBar.activeBackground": "#252525",
		"statusBar.background": "#252525",
		"button.background": "#607d8b",
		"button.foreground": "#000000",
		"input.border": "#607d8b",
		"editor.selectionBackground": "#607d8b50",
		"sideBar.border": "#607d8b0e",
		"textLink.foreground": "#dd4c35"
		
	},
	"tokenColors": [
		{
			"scope": [
				"meta.embedded",
				"source.groovy.embedded"
			],
			"settings": {
				"foreground": "#D4D4D4"
			}
		},
		{
			"scope": "emphasis",
			"settings": {
				"fontStyle": "italic"
			}
		},
		{
			"scope": "strong",
			"settings": {
				"fontStyle": "bold"
			}
		},
		{
			"scope": "header",
			"settings": {
				"foreground": "#000080"
			}
		},
		{
			"scope": "comment",
			"settings": {
				"foreground": "#5EFC8D"
			}
		},
		{
			"scope": "constant.language",
			"settings": {
				"foreground": "#569cd6"
			}
		},
		{
			"scope": [
				"constant.numeric",
				"variable.other.enummember",
