		{
			"name": "Class html",
			"scope" : "entity.other.attribute-name.html",
			"settings": {
				"foreground": "#8EF9F3",
				"fontStyle": ""
			}
		},
		{
			"name": "property ts",
			"scope" : "variable.other.property.ts",
			"settings": {
				"foreground": "#8EF9F3",
				"fontStyle": ""
			}
		},
		{
			"name": "property ts",
			"scope" : "support.function.ts",
			"settings": {
				"foreground": "#8EF9F3",
				"fontStyle": ""
			}
		}
		
		

		
		
		
		
